{
  "name": "rainwave-monokai-classic-theme",
  "description": "Theme for Rainwave based on Monokai Classic.",
  "version": "0.0.0",
  "private": true,
  "author": {
    "name": "Raine Revere",
    "email": "raine@cybersemics.org",
    "url": "https://github.com/raineorshine"
  },
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "rainwave"
  ],
  "homepage": "https://github.com/raineorshine/rainwave-monokai-classic-theme",
  "license": "ISC",
  "repository": "https://github.com/raineorshine/rainwave-monokai-classic-theme",
  "type": "module",
  "scripts": {
    "build": "bun build:style && bun build:ts",
    "build:style": "rm -f build/style.css && sass src/style.scss build/style.css && bun scripts/build-style.js",
    "build:ts": "tsc",
    "watch:style": "nodemon -w src -e ts,js,json,scss -x bun build:style",
    "watch:ts": "tsc --watch"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.248",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "sass": "^1.69.4",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "bun": "1.x"
  }
}
